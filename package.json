    "sass": "1.32.8",
    "web-vitals": "1.1.2",
    "ydb-ui-components": "2.4.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "dev": "DISABLE_ESLINT_PLUGIN=true TSC_COMPILE_ON_ERROR=true REACT_APP_BACKEND=http://localhost:8765 npm start",
    "build": "react-app-rewired build",
    "build:embedded": "rm -rf build && REACT_APP_BACKEND=http://localhost:8765 npm run build && ./prepare-build.sh",
    "lint:styles": "stylelint 'src/**/*.scss'",
    "package": "rm -rf dist && copyfiles -u 1 'src/**/*' dist",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "prepublishOnly": "npm run package",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{scss}": [
      "stylelint --fix --quiet"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --quiet"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.11",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-transition-group": "^4.4.4",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@yandex-cloud/axios-wrapper": "^1.0.2",
    "@yandex-cloud/browserslist-config": "1.0.1",
    "@yandex-cloud/eslint-config": "^1.0.0",
    "@yandex-cloud/prettier-config": "^1.0.0",
    "@yandex-cloud/stylelint-config": "^1.1.0",
    "@yandex-cloud/tsconfig": "^1.0.0",
    "@yandex-cloud/uikit": "^2.8.0",
    "copyfiles": "^2.4.1",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-app-rewired": "^2.1.11",
    "react-dom": "^17.0.2",
    "stylelint": "^14.3.0",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "@yandex-cloud/browserslist-config": "^1.0.1",
    "@yandex-cloud/uikit": "^2.8.0"
  }
}
