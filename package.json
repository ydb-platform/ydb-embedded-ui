{
  "name": "ydb-embedded-ui",
  "version": "3.2.3",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:ydb-platform/ydb-embedded-ui.git"
  },
  "dependencies": {
    "@gravity-ui/axios-wrapper": "^1.3.0",
    "@gravity-ui/i18n": "^1.0.0",
    "@gravity-ui/navigation": "^0.3.1",
    "@yandex-cloud/paranoid": "^1.3.0",
    "@yandex-cloud/react-data-table": "^1.0.2",
    "axios": "0.19.2",
    "bem-cn-lite": "4.0.0",
    "history": "4.10.1",
    "js-cookie": "2.2.1",
    "keymaster": "1.6.2",
    "lodash": "4.17.11",
    "monaco-editor": "0.24.0",
    "numeral": "2.0.6",
    "path-to-regexp": "3.0.0",
    "react-json-inspector": "7.1.1",
    "react-list": "0.8.11",
    "react-monaco-editor": "0.30.1",
    "react-redux": "7.2.6",
    "react-router": "5.2.1",
    "react-router-dom": "5.3.0",
    "react-scripts": "4.0.3",
    "react-split": "2.0.14",
    "react-transition-group": "4.4.2",
    "redux": "4.0.1",
    "redux-location-state": "2.6.0",
    "redux-thunk": "2.3.0",
    "reselect": "4.1.6",
    "sass": "1.32.8",
    "web-vitals": "1.1.2",
    "ydb-ui-components": "^3.0.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "dev": "DISABLE_ESLINT_PLUGIN=true TSC_COMPILE_ON_ERROR=true REACT_APP_BACKEND=http://localhost:8765 npm start",
    "build": "react-app-rewired build",
    "//build:embedded": "echo 'PUBLIC_URL is a setting for create-react-app. Embedded version is built and hosted as is on ydb servers, with no way of knowing the final URL pattern. PUBLIC_URL=. keeps paths to all static relative, allowing servers to handle them as needed'",
    "build:embedded": "rm -rf build && PUBLIC_URL=. REACT_APP_BACKEND=http://localhost:8765 npm run build",
    "lint:styles": "stylelint 'src/**/*.scss'",
    "package": "rm -rf dist && copyfiles -u 1 'src/**/*' dist",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "prepublishOnly": "npm run package",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{scss}": [
      "stylelint --fix --quiet"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --quiet"
    ]
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "rootDir": ".",
    "transform": {
      "^.+\\.[jt]sx?$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "jest-transform-css"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@gravity-ui/eslint-config": "^1.0.2",
    "@gravity-ui/prettier-config": "^1.0.1",
    "@gravity-ui/stylelint-config": "^1.0.1",
    "@gravity-ui/tsconfig": "^1.0.0",
    "@gravity-ui/uikit": "^3.0.1",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/lodash": "^4.14.178",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.11",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-transition-group": "^4.4.4",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "copyfiles": "^2.4.1",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "jest-transform-css": "^4.0.1",
    "lint-staged": "^12.3.7",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-app-rewired": "^2.1.11",
    "react-dom": "^17.0.2",
    "stylelint": "^14.3.0",
    "ts-jest": "^28.0.7",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "@gravity-ui/uikit": "^3.0.1"
  }
}
