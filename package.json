    "bem-cn-lite": "4.0.0",
    "history": "4.10.1",
    "js-cookie": "2.2.1",
    "keymaster": "1.6.2",
    "lodash": "4.17.11",
    "monaco-editor": "0.24.0",
    "numeral": "2.0.6",
    "path-to-regexp": "3.0.0",
    "react-json-inspector": "7.1.1",
    "react-list": "0.8.11",
    "react-monaco-editor": "0.30.1",
    "react-redux": "7.2.6",
    "react-router": "5.2.1",
    "react-router-dom": "5.3.0",
    "react-scripts": "4.0.3",
    "react-split": "2.0.14",
    "react-transition-group": "4.4.2",
    "redux": "4.0.1",
    "redux-location-state": "2.6.0",
